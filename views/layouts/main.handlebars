    <script type="module" src="{{this}}"></script>
    {{/each}}
</head>

<body>
    <!-- **>** est pour aller dans le folder **partials**-->
    {{>header}}

    <main>
        <!--Dans le folder layouts-->
        {{{body}}}
    </main>
    <div class="footer">
        <footer>
            {{>footer}}
        </footer>
    </div>

</body>

</html>